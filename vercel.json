{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/sign/(.*)", "dest": "/api/index.js" },
    { "src": "/storage/(.*)", "dest": "/api/index.js" },
    { "src": "^/$", "dest": "/web/form.html" },
    { "src": "(.*)", "dest": "/web/$1" }
  ]
}
